(/rl/)

::std [A: TYPE, B: TYPE] Pair
{
	CONSTRUCTOR();
	CONSTRUCTOR(f: A#&, s: B#&) INLINE:
		First(f), Second(s);
	CONSTRUCTOR(rhs: [A,B]Pair#&) INLINE:
		First(rhs.First), Second(rhs.Second);
	CONSTRUCTOR(rhs: [A#,B#]Pair#&) INLINE:
		First(rhs.First), Second(rhs.Second);
	First: A;
	Second: B;
}

::std [A: TYPE, B: TYPE] pair(
	a: A!&&,
	b: B!&&)
	::= [A,B]Pair(a, b);