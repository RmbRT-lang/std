::std TYPE Size := ::size_t;
::std TYPE SSize := ::__cpp_std::[Size]make_signed_t;
::std TYPE Index := ::size_t;

::std TYPE Int8 := ::int8_t;
::std TYPE UInt8 := ::uint8_t;
::std TYPE Int16 := ::int16_t;
::std TYPE UInt16 := ::uint16_t;
::std TYPE Int32 := ::int32_t;
::std TYPE UInt32 := ::uint32_t;
::std TYPE Int64 := ::int64_t;
::std TYPE UInt64 := ::uint64_t;

::std TYPE C8 := ::char;
::std TYPE C16 := UInt16;
::std TYPE C32 := UInt32;
::std TYPE Char := UInt32;

